# Predixen.app Environment Variables
# Copy to .env and fill in values

# Server
PORT=3000
NODE_ENV=development

# n8n Webhooks
N8N_BASE_URL=https://vysheshk.app.n8n.cloud
N8N_DECISION_WEBHOOK=/webhook/api/decision
N8N_FOUNDER_WEBHOOK=/webhook/founder-panel
N8N_CODE_PIPELINE_WEBHOOK=/webhook/code-pipeline

# OpenAI
OPENAI_API_KEY=sk-your-key-here

# GitHub
GITHUB_TOKEN=ghp-your-token-here
GITHUB_OWNER=visheshkhurana
GITHUB_REPO=predixen-smoke-tests

# Database (PostgreSQL/Supabase)
DATABASE_URL=postgresql://user:password@host:5432/predixen
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key

# Autonomy Settings
DEFAULT_AUTONOMY_LEVEL=supervised
RISK_THRESHOLD_FOR_APPROVAL=HIGH
