{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "request_id",
    "analysis_type",
    "product_data"
  ],
  "properties": {
    "request_id": {
      "type": "string"
    },
    "analysis_type": {
      "type": "string",
      "enum": [
        "feature_priority",
        "roadmap_review",
        "user_feedback",
        "competitive_analysis"
      ]
    },
    "product_data": {
      "type": "object"
    },
    "user_metrics": {
      "type": "object"
    }
  }
}